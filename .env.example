# Data directory path
DATA_PATH=/data/

# Database file name (will be created in DATA_PATH)
DB_FILE=cartracker.db

# Geocache file name (will be created in DATA_PATH)
GEOCACHE=geocache.json

# Upload directory name (will be created in DATA_PATH)
UPLOAD_DIR=uploads

# Gemini AI API key for receipt parsing
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini model to use for OCR
GEMINI_MODEL=gemini-1.5-flash

# Server port
PORT=8080