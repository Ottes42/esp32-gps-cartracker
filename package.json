{
  "name": "gps-car-tracker",
  "version": "1.0.0",
  "description": "GPS car tracker with fuel consumption tracking",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --env-file=.env --env-file=.env.local --watch index.js",
    "dev:docker": "node --env-file=.env --watch index.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "testCsv": "node scripts/generateTestCSV.js",
    "testFuel": "node scripts/generateFuelData.js",
    "prepare": "command -v husky >/dev/null 2>&1 && husky install || true"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.15.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "standard": "^17.1.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "gps",
    "tracking",
    "car",
    "fuel",
    "esphome",
    "iot"
  ],
  "author": "Michael Freund <ottes@ottes-techno.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ottes42/gps-car-tracker.git"
  }
}
